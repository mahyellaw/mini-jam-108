config_version=4

_global_script_classes=[ {
"base": "Sprite",
"class": "ShopBlueprint",
"language": "GDScript",
"path": "res://utils/ShopBlueprint.gd"
} ]
_global_script_class_icons={
"ShopBlueprint": ""
}

[application]

config/name="One Stop - One Shop"
run/main_scene="res://scenes/World.tscn"
